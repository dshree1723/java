import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		if(n%2==0&&n%5==0&&n%8!=0){
		System.out.println("divisible by 2 and 5 but not by 8");}
		else{
		System.out.println("not divisible by 2 and 5 but not by 8");}
	}
}
