import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
        int c=a*b;
        if(c>0){
        System.out.println("positive");}
        else if(c<0){
            System.out.println("nrgative");}
        else{
            System.out.println("zero");}
	}
}
