import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int sumsq=0;int sumcu=0;
		int temp=n;
		while(temp>0){
		    int digit=temp%10;
		    sumsq+=digit*digit;
		    sumcu+=digit*digit*digit;
		    temp/=10;
		}
		System.out.println("sqaure:"+sumsq);
		System.out.println("cube:"+sumcu);
	}
}
