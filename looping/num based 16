import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int sum=0;
		int square=n*n;
		while(square>0){
		    int digit=square%10;
		    sum=sum+digit;
		    square=square/10;
		}
		if(sum==n){
		    System.out.println("neon number");
		}else{
		 System.out.println("not a neon number");   
		}
	}
}
