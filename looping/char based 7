import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int ones=0;
		int zeros=0;
		while(n>0){
		    int digit=n%10;
		    if(digit==1){
		      ones++;
		    }else if(digit==0){
		        zeros++;
		    }
		    n=n/10;
		}
		System.out.println("1s:"+ones);
		System.out.println("0s:"+zeros);
	}
}
