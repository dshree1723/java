import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int root=(int)Math.sqrt(n);
		if(root*root==n){
		    System.out.println("perfect square");
		}else{
		    System.out.println("not a perfect square");
		}
	}
}
