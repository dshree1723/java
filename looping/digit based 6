import java.util.*;
public class Main
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   int n=sc.nextInt();
	    int prev=n%10;
	    n=n/10;
	    boolean descending =true;
	    while(n>0){
	        int current=n%10;
	        if(current<prev){
	            descending=false;
	            break;
	        }
	        prev=current;
	        n=n/10;
	    }if(descending){
	        System.out.println("descending order");
	    }else{
	        System.out.println("not descening order");
	    }
	}
}
