import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int countprime=0;
		int num=2;
		while(countprime<n){
		    int count=0;
		for(int i=2;i <= num/2;i++){
		    if(num%i==0){
		        count++;
		        break;
		    }
		}
		if(count==0){
		    System.out.print(num+" ");
		    countprime++;
		}
		num++;
	}
}
}
