import java.util.*;
public class Main
{
	public static void main(String[] args) {
	  Scanner sc=new Scanner(System.in);
	  int n=sc.nextInt();
	  int temp=n;
	  int product=1;
	  while(temp>1){
	      int digit=temp%10;
	      product=product*digit;
	      temp=temp/10;
	  }
	  System.out.println(product);
	}
}
