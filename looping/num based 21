import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		if(n<=0){
		   System.out.println("not a power of 3");
		    return;
		}
		while(n%3==0){
		    n=n/3;
		}
		if(n==1){
		    System.out.println("power of 3");
		}else{
		    System.out.println("not a power of 3");
		}
	}
}
