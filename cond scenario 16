import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
	     String mnumber=sc.next();
	     if(mnumber.length()==10){
	         System.out.println("valid number");
	     }
	     else{
	         System.out.println("invalid number");
	     }
	}
}
