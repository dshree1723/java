import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
	     int age=sc.nextInt();
	     if(age<12){
	         System.out.println("child ticket");
	     }
	     else if(age<60){
	         System.out.println("adult ticket");
	     }
	     else{
	         System.out.println("senior citizen");
	     }
	  }     
}
